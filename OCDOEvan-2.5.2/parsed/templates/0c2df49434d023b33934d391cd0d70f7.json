{
  "pages/gift-certificate/purchase": "{{#partial \"page\"}}\n\n{{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\n<h1 class=\"page-heading\">{{lang 'gift_certificate.heading' }}</h1>\n{{> components/gift-certificate/navigation gift_page='purchase'}}\n\n<main class=\"account account--fixed\">\n\n    {{#each forms.gift_certificate.errors}}\n        {{> components/common/alert-error this}}\n    {{/each}}\n\n    <form class=\"form gift-certificate-form\" method=\"post\" id=\"gift-certificate-form\" action=\"{{forms.gift_certificate.action}}\">\n        {{#if forms.gift_certificate.cart_item_id}}\n            <input type=\"hidden\" name=\"cartitemid\" value=\"{{forms.gift_certificate.cart_item_id}}\">\n        {{/if}}\n        <div class=\"form-row form-row--half\">\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"from_name\">\n                    {{lang 'forms.gift_certificate.purchase.from_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"from_name\" id=\"from_name\" value=\"{{forms.gift_certificate.values.from_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"from_email\">\n                    {{lang 'forms.gift_certificate.purchase.from_email' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"from_email\" id=\"from_email\" value=\"{{forms.gift_certificate.values.from_email}}\">\n            </div>\n        </div>\n        <div class=\"form-row form-row--half\">\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"to_name\">\n                    {{lang 'forms.gift_certificate.purchase.to_name' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"to_name\" id=\"to_name\" value=\"{{forms.gift_certificate.values.to_name}}\">\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"to_email\">\n                    {{lang 'forms.gift_certificate.purchase.to_email' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <input type=\"text\" class=\"form-input\" name=\"to_email\" id=\"to_email\" value=\"{{forms.gift_certificate.values.to_email}}\">\n            </div>\n        </div>\n        <div class=\"form-row\">\n            <fieldset class=\"form-fieldset form-column\">\n                {{#if forms.gift_certificate.can_use_custom_amount }}\n                    <div class=\"form-field\">\n                        <label class=\"form-label\" for=\"certificate_amount\">\n                            {{lang 'forms.gift_certificate.purchase.amount' }}\n                            <small>{{lang 'common.required' }}</small>\n                        </label>\n                        <input type=\"text\" data-min=\"{{forms.gift_certificate.minimum.value}}\" data-max=\"{{forms.gift_certificate.maximum.value}}\" data-min-formatted=\"{{forms.gift_certificate.minimum.formatted}}\" data-max-formatted=\"{{forms.gift_certificate.maximum.formatted}}\" class=\"form-input\" name=\"certificate_amount\" id=\"certificate_amount\" value=\"{{forms.gift_certificate.values.amount.value}}\">\n                    </div>\n                {{else}}\n                    <div class=\"form-field\">\n                        <label class=\"form-label\" for=\"selected_amount\">\n                            {{lang 'forms.gift_certificate.purchase.amount' }}\n                            <small>{{lang 'common.required' }}</small>\n                        </label>\n                        <select class=\"form-select\" name=\"selected_amount\" id=\"selected_amount\">\n                            {{#each forms.gift_certificate.amount_options}}\n                                <option value=\"{{value}}\">{{formatted}}</option>\n                            {{/each}}\n                        </select>\n                    </div>\n                {{/if}}\n                {{#if forms.gift_certificate.expires_in_days }}\n                    <div class=\"form-field\">\n                        <input type=\"checkbox\" class=\"form-checkbox\" name=\"agree\" id=\"agree\">\n                        <label class=\"form-label\" for=\"agree\">{{lang 'forms.gift_certificate.purchase.agree' days=forms.gift_certificate.expires_in_days}}</label>\n                    </div>\n                {{/if}}\n                <div class=\"form-field\">\n                    <input type=\"checkbox\" class=\"form-checkbox\" name=\"agree2\" id=\"agree2\"{{#if forms.gift_certificate.editing}} checked{{/if}}>\n                    <label class=\"form-label\" for=\"agree2\">{{lang 'forms.gift_certificate.purchase.agree2' }}</label>\n                </div>\n            </fieldset>\n            <fieldset class=\"form-fieldset form-column\">\n                <div class=\"form-field\">\n                    <label class=\"form-label\" for=\"message\">{{lang 'forms.gift_certificate.purchase.message' }}</label>\n                    <textarea class=\"form-input\" name=\"message\" id=\"message\" rows=\"8\" >{{forms.gift_certificate.values.message}}</textarea>\n                </div>\n                <label class=\"form-label\">\n                    {{lang 'forms.gift_certificate.purchase.theme' }}\n                    <small>{{lang 'common.required' }}</small>\n                </label>\n                <div class=\"form-field form-field-group\">\n                    {{#each forms.gift_certificate.themes}}\n                        <input type=\"radio\" class=\"form-radio\" name=\"certificate_theme\" id=\"theme_{{this.value}}\" value=\"{{this.value}}\"{{#if this.value '==' ../forms.gift_certificate.values.theme}} checked{{/if}}>\n                        <label class=\"form-label\" for=\"theme_{{this.value}}\">{{this.display}}</label>\n                    {{/each}}\n                </div>\n            </fieldset>\n        </div>\n        <div class=\"form-actions\">\n            {{inject 'previewError' (lang 'forms.gift_certificate.purchase.preview_error')}}\n            {{inject 'enterAmount' (lang 'forms.validate.gift.amount')}}\n            {{inject 'toName' (lang 'forms.validate.gift.to_name')}}\n            {{inject 'toEmail' (lang 'forms.validate.gift.to_email')}}\n            {{inject 'fromName' (lang 'forms.validate.gift.from_name')}}\n            {{inject 'fromEmail' (lang 'forms.validate.gift.from_email')}}\n            {{inject 'certTheme' (lang 'forms.validate.gift.cert_theme')}}\n            {{inject 'agreeToTerms' (lang 'forms.validate.gift.agree_terms')}}\n            <input\n                id=\"gift-certificate-preview\"\n                class=\"button\"\n                type=\"submit\"\n                value=\"{{lang 'forms.gift_certificate.purchase.preview' }}\"\n                data-preview-url=\"{{forms.gift_certificate.preview}}\">\n\n            <input\n                id=\"gift-certificate-submit\"\n                class=\"button button--primary\"\n                type=\"submit\"\n                value=\"{{lang 'forms.gift_certificate.purchase.submit_value' }}\">\n        </div>\n    </form>\n</main>\n{{/partial}}\n{{> layout/base}}\n",
  "components/common/breadcrumbs": "<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\n    {{#each breadcrumbs}}\n        <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\n            {{#if url}}\n                <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\n            {{else}}\n                <meta itemprop=\"item\" content=\"{{url}}\">\n                <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\n            {{/if}}\n            <meta itemprop=\"position\" content=\"{{@index}}\" />\n        </li>\n    {{/each}}\n</ul>\n",
  "components/gift-certificate/navigation": "<nav class=\"navBar navBar--sub\">\n    <ul class=\"navBar-section account-navigation\">\n        {{#if gift_page '===' 'purchase'}}\n            <li class=\"navBar-item is-active\">\n                {{lang 'gift_certificate.purchase.heading'}}\n            </li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.purchase}}\">\n                    {{lang 'gift_certificate.purchase.heading'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if gift_page '===' 'redeem'}}\n            <li class=\"navBar-item is-active\">\n                {{lang 'gift_certificate.redeem.heading'}}\n            </li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.redeem}}\">\n                    {{lang 'gift_certificate.redeem.heading'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if gift_page '===' 'balance'}}\n            <li class=\"navBar-item is-active\">\n                {{lang 'gift_certificate.balance.heading'}}\n            </li>\n        {{else}}\n            <li class=\"navBar-item\">\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.balance}}\">\n                    {{lang 'gift_certificate.balance.heading'}}\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/alert-error": "<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n",
  "layout/base": "<!DOCTYPE html>\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\n    <head>\n        <title>{{ head.title }}</title>\n        {{{ resourceHints }}}\n        {{{ head.meta_tags }}}\n        {{{ head.config }}}\n        {{#block \"head\"}} {{/block}}\n\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\n        <link href=\"https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i|Roboto:400,400i,700,700i\" rel=\"stylesheet\">\n        <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.css\" />\n        <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\" />\n        <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css\" />\n        <script>\n            // Change document class from no-js to js so we can detect this in css\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\n        </script>\n\n        {{ getFontsCollection }}\n        {{{stylesheet '/assets/css/theme.css'}}}\n\n        {{{head.scripts}}}\n        {{{head.rsslinks}}}\n\n        {{inject 'themeSettings' theme_settings}}\n        {{inject 'genericError' (lang 'common.generic_error')}}\n        {{inject 'maintenanceMode' settings.maintenance}}\n        {{inject 'urls' urls}}\n        {{inject 'template' template}}\n        {{{snippet 'htmlhead'}}}\n        <meta name=\"google-site-verification\" content=\"H1quKkWRRR_7KeMy5nl4sYVITYk2yMW2X0xp7_4Hdns\" />\n        <!--GooGhywoiu9839t543j0s7543uw1.  Please add evan.m.maguire@gmail.com to GA account UA-125258402-1with “Manage Users and Edit” permissions - date 10/29/2020.-->\n    </head>\n    <body>\n        {{{snippet 'header'}}}\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\n\n        {{#if settings.privacy_cookie}}\n            {{> components/common/cookie}}\n        {{/if}}\n\n        {{> components/common/header }}\n        {{> components/common/body }}\n        {{> components/common/footer }}\n\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\n\n\n        <script>\n            // Exported in app.js\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\n        </script>\n\n        {{{footer.scripts}}}\n        {{{snippet 'footer'}}}\n\n\n        <a href=\"https://pcserver.ocdo.us/form.php\" id=\"open-form\" class=\"popup-form\">Request A <strong>Free Quote</strong></a>\n\n\n        <script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js\"></script>\n        <script src=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.js\"></script>\n\n        <script src=\"https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.min.js\"></script>\n\n        <script>\n            jQuery(document).ready(function(){\n                $('.logo-scroller').slick({\n                  infinite: true,\n                  slidesToShow: 7,\n                  slidesToScroll: 1,\n                  autoplay: true,\n                    autoplaySpeed: 2000,\n                    responsive: [\n                        {\n                          breakpoint: 768,\n                          settings: {\n                           slidesToShow: 3\n                          }\n                        },\n                        {\n                          breakpoint: 480,\n                          settings: {\n                            slidesToShow: 1\n                          }\n                        }\n                      ]\n                });\n                $('.testimonial_scroller').slick({\n                  infinite: true,\n                  slidesToShow: 1,\n                  slidesToScroll: 1,\n                  autoplay: true,\n                    autoplaySpeed: 3000,\n                    nextArrow:jQuery('.pnext'),\n                    prevArrow:jQuery('.pprev')\n                });\n            })\n        </script>\n        <script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery.matchHeight/0.7.2/jquery.matchHeight-min.js\"></script>\n        <script>\n            jQuery(document).ready(function(){\n                jQuery('h4.card-title').matchHeight();\n                jQuery('.card-figure').matchHeight();\n                jQuery('.card-price').matchHeight();\n            })\n        </script>\n\n                <script>\n            (function($) {\n            \t$.fn.vmenuModule = function(option) {\n            \t\tvar obj,\n            \t\t\titem;\n            \t\tvar options = $.extend({\n            \t\t\t\tSpeed: 220,\n            \t\t\t\tautostart: true,\n            \t\t\t\tautohide: 1\n            \t\t\t},\n            \t\t\toption);\n            \t\tobj = $(this);\n\n            \t\titem = obj.find(\"ul\").parent(\"li\").children(\"a\");\n            \t\titem.attr(\"data-option\", \"off\");\n\n            \t\titem.unbind('click').on(\"click\", function( event ) {\n            \t\t    event.preventDefault();\n            \t\t\tvar a = $(this);\n            \t\t\tif (options.autohide) {\n            \t\t\t\ta.parent().parent().find(\"a[data-option='on']\").parent(\"li\").children(\"ul\").slideUp(options.Speed / 1.2,\n            \t\t\t\t\tfunction() {\n            \t\t\t\t\t\t$(this).parent(\"li\").children(\"a\").attr(\"data-option\", \"off\");\n            \t\t\t\t\t})\n            \t\t\t}\n            \t\t\tif (a.attr(\"data-option\") == \"off\") {\n            \t\t\t\ta.parent(\"li\").children(\"ul\").slideDown(options.Speed,\n            \t\t\t\t\tfunction() {\n            \t\t\t\t\t\ta.attr(\"data-option\", \"on\");\n            \t\t\t\t\t});\n            \t\t\t}\n            \t\t\tif (a.attr(\"data-option\") == \"on\") {\n            \t\t\t\ta.attr(\"data-option\", \"off\");\n            \t\t\t\ta.parent(\"li\").children(\"ul\").slideUp(options.Speed)\n            \t\t\t}\n            \t\t});\n            \t\tif (options.autostart) {\n            \t\t\tobj.find(\"a\").each(function() {\n\n            \t\t\t\t$(this).parent(\"li\").parent(\"ul\").slideDown(options.Speed,\n            \t\t\t\t\tfunction() {\n            \t\t\t\t\t\t$(this).parent(\"li\").children(\"a\").attr(\"data-option\", \"on\");\n            \t\t\t\t\t})\n            \t\t\t})\n            \t\t}\n\n            \t}\n            })(window.jQuery || window.Zepto);\n        </script>\n\n        <script>\n            jQuery(document).ready(function(){\n                jQuery(\".u-vmenu\").vmenuModule({ Speed: 200, autostart: false, autohide: true });\n                jQuery('.u-vmenu > ul > li > a').click(function(){\n                    jQuery(this).parent().siblings().removeClass('sactive');\n                })\n            })\n\n            $(document).ready(function() {\n            \t$('.popup-youtube, .popup-vimeo, .popup-gmaps, .popup-form').magnificPopup({\n            \t\tdisableOn: 700,\n            \t\ttype: 'iframe',\n            \t\tmainClass: 'mfp-fade',\n            \t\tremovalDelay: 160,\n            \t\tpreloader: false,\n\n            \t\tfixedContentPos: false\n            \t});\n            });\n\n        </script>\n\n\n\n\n\n    </body>\n</html>\n",
  "components/common/header": "{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    <div class=\"banner\">\n        {{{limit banners.top 1}}}\n    </div>\n</div>\n{{/if}}\n\n<div class=\"topBar\">\n    <div class=\"container\">\n        <ul class=\"linkLeft\">\n            <li><a href=\"/shipping-information/\">SHIPPING</a></li>\n            <li><a href=\"/warranty/\">WARRANTY</a></li>\n            <li><a href=\"/contact-us/\">contact us</a></li>\n            <li><a href=\"/faq/\">faq</a></li>\n            <li><a href=\"/troubleshooting/\">technical issues</a></li>\n        </ul>\n        \n        \n        {{> components/common/navigation}}\n        \n        \n    </div>\n</div>\n\n<header class=\"header\" role=\"banner\">\n    \n    \n    <div class=\"logo-search container\">\n\n    {{#if page_type '===' 'default'}}\n        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </h1>\n    {{else}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </div>\n    {{/if}}\n    \n    {{> components/common/search-box}}\n    \n    </div>\n    \n    {{{region name=\"header_bottom\"}}}\n    \n    <div class=\"mobile-navigation container\">\n        <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n            <span class=\"mobileMenu-toggleIcon\"> </span><span class=\"menuText\">MENU</span>\n        </a>\n        <a href=\"/cart.php\" id=\"mobileCarte\">\n            <i class=\"fa fa-shopping-cart\"></i> <span class=\"cart-quantity\">{{cart.quantity}} Item(s), {{cart.sub_total.formatted}}</span>\n        </a>\n    </div>\n    \n    \n    \n    <div class=\"container\" id=\"menu\" data-menu>\n        \n        {{> components/common/navigation-menu}}\n    </div>\n</header>\n \n",
  "components/common/cookie": "<div class=\"cookieMessage\">\n    <div class=\"cookieMessage-container\">\n        <div class=\"cookieMessage-text\">\n            <p>{{settings.privacy_cookie}}</p>\n        </div>\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\n    </div>\n</div>\n",
  "components/common/body": "<div class=\"body\">\n    {{#block \"hero\"}} {{/block}}\n    <div class=\"container\">\n        {{#block \"page\"}} {{/block}}\n    </div>\n    {{> components/common/modal}}\n</div>\n",
  "components/common/footer": "{{#if banners.bottom}}\n<div class=\"banners\" data-banner-location=\"bottom\">\n    <div class=\"banner\">\n        {{{limit banners.bottom 1}}}\n    </div>\n</div>\n{{/if}}\n<footer class=\"footer\" role=\"contentinfo\">\n    <div class=\"container\">\n\n        <div class=\"moreInfo\">\n            <a href=\"\" class=\"text1\">Emergency Shipping Available!</a>\n            <a href=\"\" class=\"text2\">Custom Builds available!</a>\n        </div>\n\n        <section class=\"footer-info\">\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">Customer Services</h5>\n                <ul class=\"footer-info-list\">\n                    <li><a href=\"/contact-us/\">Contact Us</a></li>\n                    <li><a href=\"/shipping-information/\">Shipping Information</a></li>\n                    <li><a href=\"/return-policy/\">Returns & Exchanges</a></li>\n                    <li><a href=\"\">Parts & Accessories</a></li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">Company Info</h5>\n                <ul class=\"footer-info-list\">\n                    <li><a href=\"/about-us/\">About Us</a></li>\n                    <li><a href=\"/privacy-policy/\">Privacy Policy</a></li>\n                    <li><a href=\"/terms-of-use/\">Terms of Use</a></li>\n                    <li><a href=\"/testimonials/\">Testimonials</a></li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">Your Account</h5>\n                <ul class=\"footer-info-list\">\n                    <li><a href=\"/account.php\">My Account</a></li>\n                    <li><a href=\"/orderstatus.php\">Order Status</a></li>\n                    <li><a href=\"/wishlist.php\">Wish List</a></li>\n                    <li><a href=\"/cart.php\">View Basket</a></li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">Get Social</h5>\n                <ul class=\"footer-info-list\">\n                    <li><a href=\"https://www.facebook.com/pcserverandparts/?ref=bookmarks\" target=\"_blank\">Facebook</a></li>\n                    <li><a href=\"https://www.instagram.com/pcserverparts/\">Instagram</a></li>\n\n                </ul>\n            </article>\n\n        </section>\n\n    </div>\n</footer>\n<div class=\"copyText\"><p>Copyright &copy; {{moment format=\"YYYY\"}} <strong>PC Server & Parts</strong>. All rights reserved. Big Commerce Themes by <a href=\"https://www.ocdesignsonline.com\" rel=\"nofollow\" target=\"_blank\">OCDesignsOnline</a>.</p></div>\n",
  "components/common/navigation": "\n    <ul class=\"navUser-section navUser-section--alt\">\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action\" href=\"/account.php\">My Account</a>\n        </li>\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action\" href=\"/orderstatus.php\">Order Status</a>\n        </li>\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            {{else}}\n                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <span class=\"cart-quantity\">{{cart.quantity}} Item(s), {{cart.sub_total.formatted}}</span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n\n",
  "components/common/search-box": "{{{ snippet \"forms_search\"}}}\n<form class=\"form\"  action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\" placeholder=\"Search our store...\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'search.results.form_button_text'}}\">\n            </div>\n        </div>\n    </fieldset>\n</form>\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    \n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        \n        \n        \n        \n        \n        \n    \n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"https://pcserverandparts.com/build-to-order-server-and-workstations/\">Shop Now</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/data-security/\">Data Security</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/it-recycling-asset-management/\">IT RECYCLING</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/certifications/\">Certifications</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/sell-your-gear/\">Sell your gear</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/business-services/\">Business Services</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/buy-from-us/\">Buy from us</a></li>\n                    <li class=\"navPages-item\"><a class=\"navPages-action\" href=\"/leasing/\">Finance Options</a></li>\n                    \n        \n\n        \n    </ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        {{#if customer}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</span></a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        {{else}}\n            <li class=\"navPages-item mobileLogin\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    {{lang 'common.or'}} <a class=\"navPages-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            </li>\n        {{/if}}\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navPages-item\">\n                {{> components/common/social-links}}\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"
